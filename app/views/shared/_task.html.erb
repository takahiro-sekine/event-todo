<div class="form-task">
  <%= form_for(@task) do |f| %>
  <div class="form-group">
    <%= f.label :task_title, "表題"%><br />
    <%= f.text_field :task_title%>
  </div>
  
  <div class="form-group">
    <%= f.label :complete, "日時", class: 'control-label' %><br />
    <%= f.date_field :complete, class: 'form-control' %>
  </div>
    <%= f.submit 'Submit' %>
  <% end %>
</div>
<p><%= link_to '一覧に戻る', tasks_url %></p>